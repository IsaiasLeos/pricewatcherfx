<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <TextFlow layoutX="14.0" layoutY="75.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="20.0"
              AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="75.0"/>
    <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
            <Menu mnemonicParsing="false" text="App">
                <items>
                    <MenuItem mnemonicParsing="false" text="About">
                        <accelerator>
                            <KeyCodeCombination alt="UP" code="A" control="DOWN" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" text="Exit">
                        <accelerator>
                            <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Item">
                <items>
                    <MenuItem mnemonicParsing="false" text="Check Price">
                        <accelerator>
                            <KeyCodeCombination alt="DOWN" code="C" control="UP" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" text="Add">
                        <accelerator>
                            <KeyCodeCombination alt="DOWN" code="A" control="UP" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" text="Search">
                        <accelerator>
                            <KeyCodeCombination alt="DOWN" code="S" control="UP" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <SeparatorMenuItem mnemonicParsing="false"/>
                    <MenuItem mnemonicParsing="false" text="Select Last">
                        <accelerator>
                            <KeyCodeCombination alt="DOWN" code="UP" control="UP" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" text="Select First">
                        <accelerator>
                            <KeyCodeCombination alt="DOWN" code="DOWN" control="UP" meta="UP" shift="UP" shortcut="UP"/>
                        </accelerator>
                    </MenuItem>
                    <Menu mnemonicParsing="false" text="Item">
                        <items>
                            <MenuItem mnemonicParsing="false" text="Refresh"/>
                            <MenuItem mnemonicParsing="false" text="Open"/>
                            <MenuItem mnemonicParsing="false" text="Delete"/>
                            <MenuItem mnemonicParsing="false" text="Edit"/>
                            <SeparatorMenuItem mnemonicParsing="false"/>
                            <MenuItem mnemonicParsing="false" text="Copy Name"/>
                            <MenuItem mnemonicParsing="false" text="Copy URL"/>
                            <MenuItem mnemonicParsing="false" text="Copy Item"/>
                        </items>
                    </Menu>
                </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Sort">
                <items>
                    <RadioMenuItem mnemonicParsing="false" text="Oldest"/>
                    <RadioMenuItem mnemonicParsing="false" text="Newest"/>
                    <RadioMenuItem mnemonicParsing="false" text="Ascending Order"/>
                    <RadioMenuItem mnemonicParsing="false" text="Descending Order"/>
                    <RadioMenuItem mnemonicParsing="false" text="Highest Price"/>
                    <RadioMenuItem mnemonicParsing="false" text="Lowest Price"/>
                    <RadioMenuItem mnemonicParsing="false" text="Highest Change"/>
                    <RadioMenuItem mnemonicParsing="false" text="Lowest Change"/>
                </items>
            </Menu>
        </menus>
    </MenuBar>
